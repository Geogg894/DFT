//method converts time domain signals to frequency domain
void calc_sig_dft(uint16_t *in_array, uint16_t *out_real, uint16_t *out_img, uint32_t array_length)
{
  int i,k,j;

  //initializing destination array elements as 0
  for(j=0;j<(array_length/2);j++)
  {
      out_real[j] =0;
      out_img[j] =0;
  }
  //place N input signal to two N/2+1 output signals
   for(k=0;k<(array_length/2);k++)
  {
    for(i=0;i<array_length;i++)
    {
               out_real[k] =  out_real[k] + in_array[i]*cos(2*PI*k*i/array_length);
               out_img[k] =  out_img[k] - in_array[i]*sin(2*PI*k*i/array_length);
    }
  }
}

//method obtains magnitudes to plot magnitude spectrum
void get_dft_output_mag(void)
{
  uint32_t k;
  for(k=0;k<44;k++)
  {
    //store magnitudes through sqrt of magnitude of real and img components
    realcomp[k] = sqrt(pow(realcomp[k],2)+pow(imgcomp[k],2));
  }
}


void plot_output(uint16_t *in_array, uint32_t array_length)
{
   for(int i=0;i<array_length;i++){
  Serial.println(in_array[i]);
  }
}
